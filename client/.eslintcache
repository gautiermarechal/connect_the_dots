          <Route path=\"/signup-success\">\n            <SignUpSuccess />\n          </Route>\n          <Route path=\"/account/:id\">\n            <AccountPage />\n          </Route>\n          <Route path=\"/connections-bookmarked/:id\">\n            <BookmarkedConnectionsPage />\n          </Route>\n          <Route path=\"/book/:id\">\n            <BookPage />\n          </Route>\n          <Route exact path=\"/connect\">\n            <PostConnection />\n          </Route>\n          <Route path=\"/connect/free\">\n            <FreeConnection />\n          </Route>\n          <Route path=\"/feed\">\n            <Feed type=\"Home\" />\n          </Route>\n          <Route path=\"/connection/:id\">\n            <SingleConnection />\n          </Route>\n        </Switch>\n        <Footer />\n      </Router>\n      <GlobalStyles />\n    </>\n  );\n}\n\nexport default App;\n",["228","229"],"/Users/gautiermarechal/connect_the_dots/client/src/components/GlobalStyles.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/NavigationBar/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/SearchBar/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/CategoriesBar/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/constants.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/HomePage/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/Connection/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/reportWebVitals.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/components/HomePage/FeedSnippet/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/redux/store.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/redux/reducers/index.js",[],"/Users/gautiermarechal/connect_the_dots/client/src/redux/reducers/BooksReducer.js",[],"/Users/gautiermarechal/connect_t